{
  "extends": "airbnb",
  "globals": {
    "document": true,
  },
  "rules": {
    "jsx-a11y/media-has-caption": 0,
    "no-unused-expressions": [2, { "allowShortCircuit": true }],
    "prefer-destructuring": 0,
    "react/destructuring-assignment": 0 ,
    "react/jsx-one-expression-per-line": 0 
  }
}
